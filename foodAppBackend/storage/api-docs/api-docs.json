    "paths": {
        "/api/questions": {
            "get": {
                "tags": [
                    "Polls"
                ],
                "summary": "Get list of questions",
                "operationId": "caa0baf44bc64c9b94a44e9ed5bc418d",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "question_text": {
                                                "type": "string",
                                                "example": "What is your favorite cuisine?"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/answers": {
            "get": {
                "tags": [
                    "Polls"
                ],
                "summary": "Get list of answers",
                "operationId": "fd2c54c83721116cef0ad4b94134932b",
