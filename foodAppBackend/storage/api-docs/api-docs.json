    "components": {
        "schemas": {
            "Restaurant": {
                "required": [
                    "name",
                    "address",
                    "city",
                    "cuisine"
                ],
                "properties": {
                    "id": {
                        "description": "Restaurant ID",
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "name": {
                        "description": "Restaurant name",
                        "type": "string",
                        "example": "Pizza Palace"
                    },
                    "address": {
                        "description": "Restaurant address",
                        "type": "string",
                        "example": "123 Main St"
                    },
                    "city": {
                        "description": "Restaurant city",
                        "type": "string",
                        "example": "New York"
                    },
                    "cuisine": {
                        "description": "Restaurant cuisine type",
                        "type": "string",
                        "example": "Italian"
                    },
                    "rating": {
                        "description": "Restaurant rating",
                        "type": "number",
                        "format": "float",
                        "example": 4.5
                    },
                    "website": {
                        "description": "Restaurant website",
                        "type": "string",
                        "example": "https://pizzapalace.com"
                    },
                    "created_at": {
                        "description": "Creation date",
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "description": "Last update date",
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            }
        },
